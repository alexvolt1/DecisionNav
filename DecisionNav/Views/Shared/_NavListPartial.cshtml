@model System.Collections.Generic.IEnumerable<DecisionNav.Models.ViewModels.NavigationListViewModel>


@foreach (var menu in Model)
{
    if (menu.Children.Any())
    {
        <li class="treeview">
            <a href="#">
                <i class="@menu.Id"></i> <span>@menu.DefaultName</span>
                <span class="pull-right-container">
                    <i class="fa fa-angle-left pull-right"></i>
                </span>
            </a>
            <ul class="treeview-menu">
                @*@Html.Partial("_TestPartial", menu.Children)*@
                <partial name="_NavListPartial" model="menu.Children" />
            </ul>
        </li>
    }
    else
    {
        <li class="treeview">
            <a href="@menu.ImageUrl">
                <i class="@menu.Id"></i> <span>@menu.DefaultName</span>
            </a>
        </li>
    }
}

