@model System.Collections.Generic.IEnumerable<DecisionNav.Models.ViewModels.NavigationListViewModel>


@foreach (var menu in Model)
{
    if (menu.Children.Any())
    {
        <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown-@menu.Id" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <span>@menu.DefaultName</span>
            </a>
            <ul class="dropdown-menu" aria-labelledby="pagesDropdown-@menu.Id">
                @*@Html.Partial("_TestPartial", menu.Children)*@
                <partial name="_NavListPartial" model="menu.Children" />
            </ul>
        </li>
    }
    else
    {
        <li class="nav-item dropdown">
            <a class="nav-link" href="@menu.ViewId" id="pagesDropdown-@menu.Id" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <span>@menu.DefaultName</span>
            </a>
        </li>
    }
}

